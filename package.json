{
  "name": "pooh.js",
  "version": "1.0.0",
  "description": "A honeypot for forms.",
  "main": "pooh.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-modern": "BABEL_ENV=modern npx babel src/pooh.js --out-file build/pooh.js",
    "build-es5": "BABEL_ENV=es5 npx babel src/pooh.js --out-file build/pooh.es5.js",
    "build": "npm run build-modern && npm run build-es5"
  },
  "repository": "git+https://github.com/BuddyLReno/poohjs.git",
  "keywords": [
    "honeypot",
    "honey",
    "honey pot",
    "spam",
    "spam checker"
  ],
  "author": "Buddy Reno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BuddyLReno/poohjs/issues"
  },
  "homepage": "https://github.com/BuddyLReno/poohjs#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-modern-browsers": "^12.0.0-beta.0"
  }
}
